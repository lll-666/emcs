<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.emcs.mapper.OneTableSelectMapper" >



    <select id="selectVirAcctType" resultType="hashMap" >
        select
        *
        from va_virtual_acct_type
    </select>

    <select id="selectByPayMerchId" parameterType="string" resultType="hashMap">
        select
        pay_merch_id
        from va_plat_info
        where
        pay_merch_id = #{payMerchId}
    </select>

</mapper>